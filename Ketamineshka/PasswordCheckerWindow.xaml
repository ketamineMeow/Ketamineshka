<Window x:Class="YourNamespace.PasswordCheckerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Проверка Пароля" Height="200" Width="300">
    <StackPanel>
        <TextBlock Text="Введите пароль : " Margin="10"/>
        <TextBox x:Name="PasswordTextBox" Margin="10" />
        <Button Content="Проверить" Click="CheckPassword"/>
        <TextBlock x:Name="ResultTextBlock" Margin="10"/>
    </StackPanel>
</Window>

